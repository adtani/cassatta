#!/bin/bash
echo "Custom Build Script Executing Now"
mvn -v
echo *** Environment Variables ***
env | grep OPENSHIFT
echo *** Launching MVN ***
echo cd $OPENSHIFT_HOMEDIR/app-root/diy/repo
cd $OPENSHIFT_HOMEDIR/app-root/diy/repo
echo mvn package -s .openshift/settings.xml -DskipTests=true
mvn package -s .openshift/settings.xml -DskipTests=true
echo "Custom Build Script Execution Completed..!"